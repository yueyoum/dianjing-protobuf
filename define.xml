<namespace name="protocol">
    <file name="server">
        <protocol name="GetServerListRequest" type="100" command="/game/servers/" />
        <protocol name="GetServerListResponse" type="101" />
        <protocol name="StartGameRequest" type="102" command="/game/start/" />
        <protocol name="StartGameResponse" type="103" />
    </file>
    
    <file name="account">
        <protocol name="RegisterRequest" type="200" command="/game/account/register/" />
        <protocol name="RegisterResponse" type="201" />
        <protocol name="LoginRequest" type="202" command="/game/account/login/" />
        <protocol name="LoginResponse" type="203" />
    </file>
    
    <file name="character">
        <protocol name="CharacterNotify" type="300" />
        <protocol name="CreateCharacterRequest" type="301" command="/game/character/create/" />
        <protocol name="CreateCharacterResponse" type="302" />
    </file>
    
    <file name="club">
        <protocol name="ClubNotify" type="400" />
        <protocol name="CreateClubRequest" type="401" command="/game/club/create/" />
        <protocol name="CreateClubResponse" type="402" />
    </file>
</namespace>
