<namespace name="protocol">
    <file name="common">
        <protocol name="UTCNotify" type="10" />
    </file>

    <file name="server">
        <protocol name="GetServerListRequest" type="100" command="/game/servers/" />
        <protocol name="GetServerListResponse" type="101" reqtype="100" />
        <protocol name="StartGameRequest" type="102" command="/game/start/" />
        <protocol name="StartGameResponse" type="103" reqtype="102" />
    </file>
    
    <file name="account">
        <protocol name="RegisterRequest" type="200" command="/game/account/register/" />
        <protocol name="RegisterResponse" type="201" reqtype="200" />
        <protocol name="LoginRequest" type="202" command="/game/account/login/" />
        <protocol name="LoginResponse" type="203" reqtype="202" />
    </file>
    
    <file name="character">
        <protocol name="CharacterNotify" type="300" />
        <protocol name="CreateCharacterRequest" type="301" command="/game/character/create/" />
        <protocol name="CreateCharacterResponse" type="302" reqtype="301" />
    </file>
    
    <file name="club">
        <protocol name="ClubNotify" type="400" />
        <protocol name="CreateClubRequest" type="401" command="/game/club/create/" />
        <protocol name="CreateClubResponse" type="402" reqtype="401" />

        <protocol name="ClubSetPolicyRequest" type="410" command="/game/club/policy/" />
        <protocol name="ClubSetPolicyResponse" type="411" reqtype="410" />
        <protocol name="ClubSetMatchStaffRequest" type="412" command="/game/club/matchstaff/" />
        <protocol name="ClubSetMatchStaffResponse" type="413" reqtype="412" />
    </file>

    <file name="staff">
        <protocol name="StaffNotify" type="500" />
        <protocol name="StaffRemoveNotify" type="501" />
        <protocol name="StaffTrainingRequest" type="502" command="/game/staff/training/" />
        <protocol name="StaffTrainingResponse" type="503" reqtype="502" />
        <protocol name="StaffTrainingGetRewardRequest" type="504" command="/game/staff/training/getreward/" />
        <protocol name="StaffTrainingGetRewardResponse" type="505" reqtype="504" />

        <protocol name="StaffRecruitNotify" type="520" />
        <protocol name="StaffRecruitRefreshRequest" type="521" command="/game/staff/recruit/refresh/" />
        <protocol name="StaffRecruitRefreshResponse" type="522" reqtype="521" />
        <protocol name="StaffRecruitRequest" type="523" command="/game/staff/recruit/" />
        <protocol name="StaffRecruitResponse" type="524" reqtype="523" />
    </file>

    <file name="league">
        <protocol name="LeagueNotify" type="600" />
        <protocol name="LeagueGetStatisticsRequest" type="601" command="/game/league/statistics/" />
        <protocol name="LeagueGetStatisticsResponse" type="602" reqtype="601" />
        <protocol name="LeagueGetBattleLogRequest" type="603" command="/game/league/log/" />
        <protocol name="LeagueGetBattleLogResponse" type="604" reqtype="603" />
    </file>

    <file name="challenge">
        <protocol name="ChallengeNotify" type="700" />
        <protocol name="ChallengeStartRequest" type="701" command="/game/challenge/start/" />
        <protocol name="ChallengeStartResponse" type="702" reqtype="701" />
    </file>
</namespace>
