<namespace name="protocol">
    <namespace name="server">
        <protocol name="GetServerListRequest" type="100" command="/servers/" />
        <protocol name="GetServerListResponse" type="101" />
        <protocol name="StartGameRequest" type="102" command="/start/" />
        <protocol name="StartGameResponse" type="103" />
    </namespace>
    
    <namespace name="account">
        <protocol name="RegisterRequest" type="200" command="/account/register/" />
        <protocol name="RegisterResponse" type="201" />
        <protocol name="LoginRequest" type="202" command="/account/login/" />
        <protocol name="LoginResponse" type="203" />
    </namespace>
    
    <namespace name="character">
        <protocol name="CharacterNotify" type="300" />
        <protocol name="CreateCharacterRequest" type="301" command="/character/create/" />
        <protocol name="CreateCharacterResponse" type="302" />
    </namespace>
    
    <namespace name="club">
        <protocol name="ClubNotify" type="400" />
        <protocol name="CreateClubRequest" type="401" command="/club/create/" />
        <protocol name="CreateClubResponse" type="402" />
    </namespace>
</namespace>
